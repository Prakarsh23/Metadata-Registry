{
  "name": "jonphipps/metadata-registry",
  "description": "A metadata registry",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:jonphipps/phpspecgen.git"
    }
  ],
  "require-dev": {
    "kint-php/kint": "^2.2",
    "phpspec/phpspec": "^2.1.0",
    "henrikbjorn/phpspec-code-coverage": "^1.0",
    "codeception/specify": "^0.4.3",
    "codeception/verify": "^0.3.0",
    "captbaritone/mailcatcher-codeception-module": "^1.1",
    "codeception/codeception": "^2.2"
  },
  "config": {
  "process-timeout"    : 900,
  "cache-dir:"         : "../",
  "preferred-install"  : "dist",
  "sort-packages"      : true,
  "optimize-autoloader": true
},
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Jon Phipps",
      "email": "jphipps@madcreek.com"
    }
  ],
  "minimum-stability": "beta",
  "require": {
    "bgcc/sf-select2-widgets-plugin": "~1.0",
    "ddeboer/data-import": "0.18.*",
    "easyrdf/easyrdf": "^0.9.1",
    "google/apiclient": "^1.1",
    "guzzlehttp/guzzle": "^6.3",
    "league/flysystem": "0.5.*",
    "league/plates": "2.*",
    "mjphaynes/php-resque": "^1.2",
    "ml/json-ld": "^1.0",
    "neitanod/forceutf8": "^2.0",
    "phpoffice/phpspreadsheet": "^1.1",
    "psr/log": "^1.0",
    "robmorgan/phinx": "^0.5.0",
    "semsol/arc2": "^2.2",
    "symfony/http-foundation": "^2.7.0",
    "symfony/intl": "^2.7.0",
    "symfony/property-access": "^2.7.0",
    "symfony/validator": "^2.7.0",
    "symfony/var-dumper": "^2.8.0",
    "vlucas/phpdotenv": "^2.0"
  },

  "autoload": {
    "psr-4": {
      "ImportVocab\\": "plugins/jpAdminGeneratorPlugin/lib/ImportVocab",
      "model\\": "lib/model",
      "apps\\": "apps"
    },
    "classmap": ["lib/model/",
        "tests"
    ]
  },

    "include-path": ["lib/model"]

}
