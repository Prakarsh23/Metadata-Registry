{
  "name": "jonphipps/metadata-registry",
  "description": "A metadata registry",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:jonphipps/phpspecgen.git"
    }
  ],
  "require-dev": {
    "codeception/codeception": "2.0.x",
    "codeception/specify": "*",
    "codeception/verify": "*",
    "codeception/aspect-mock": "*",
    "codeception/phpbuiltinserver": "*",
    "captbaritone/mailcatcher-codeception-module": "*",
    "raveren/kint": "1.0.*@dev",
    "phpspec/phpspec": "2.1.0",
    "vlucas/phpdotenv": "1.*@dev"
  },
  "config": {
    "process-timeout": 900,
    "cache-dir:": "../"
  },
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Jon Phipps",
      "email": "jphipps@madcreek.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "zendframework/zendframework1": "1.12.*@dev",
    "google/apiclient": "dev-master",
    "semsol/arc2": "*",
    "easyrdf/easyrdf": "dev-master",
    "ml/json-ld": "dev-master",
    "ddeboer/data-import": "*",
    "symfony/property-access": "*",
    "phpoffice/phpexcel": "*",
    "symfony/validator": "*",
    "symfony/config": "*",
    "symfony/intl": "*",
    "psr/log": "dev-master",
    "symfony/http-foundation": "*",
    "bgcc/sf-select2-widgets-plugin": "~1.0",
    "league/plates": "2.*",
    "league/flysystem": "0.5.*"
  },

  "autoload": {
    "psr-4": {
      "ImportVocab\\": "plugins/jpAdminGeneratorPlugin/lib/ImportVocab",
      "model\\": "lib/model"
    },
    "classmap": ["lib/model/"]
  },

    "include-path": ["lib/model"]

}
