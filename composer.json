{
    "name": "jonphipps/metadata-registry",
    "description": "A metadata registry",
    "repositories": [
    {
        "type": "vcs",
        "url": "git@github.com:jonphipps/phpspecgen.git"
    },
    {
        "type": "vcs",
        "url": "git@github.com:jonphipps/laxative.git"
    },
    {
        "type": "vcs",
        "url": "git@github.com:jonphipps/html.git"
    },
    {
        "type": "vcs",
        "url": "git@github.com:jonphipps/laravel-codeception-dbdump.git"
    },
    {
        "type": "vcs",
        "url": "git@github.com:jonphipps/laravel-wizard.git"
    }],
    "config":
    {
        "process-timeout": 900,
        "cache-dir:": "../",
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "license": "Apache-2.0",
    "authors": [
    {
        "name": "Jon Phipps",
        "email": "jphipps@madcreek.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require":
    {
        "php": ">=7.0.0",
        "arcanedev/log-viewer": "~4.0",
        "arcanedev/no-captcha": "^5.0",
        "asimlqt/php-google-spreadsheet-client": "3.0.*",
        "backpack/base": "^0.7.16",
        "backpack/crud": "^3.2",
        "barryvdh/laravel-snappy": "^0.4.0",
        "bgcc/sf-select2-widgets-plugin": "~1.0",
        "bpocallaghan/titan": "^0.2",
        "bugsnag/bugsnag": "^3.0",
        "bugsnag/bugsnag-laravel": "^2.0",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "creativeorange/gravatar": "^1.0",
        "ddeboer/data-import": "0.18.*",
        "doctrine/dbal": "^2.6",
        "easyrdf/easyrdf": "^0.9.1",
        "fideloper/proxy": "~3.3",
        "google/apiclient": "^2.1",
        "graham-campbell/github": "^6.0",
        "guzzlehttp/guzzle": "^6.3",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "hashids/hashids": "^2.0",
        "hieu-le/active": "~3.0",
        "ixudra/curl": "^6.10",
        "ixudra/generators": "^5.6",
        "ixudra/render": "^0.1.0",
        "jeroen-g/laravel-packager": "^1.5",
        "laracasts/matryoshka": "dev-master",
        "laravel/framework": "5.5.*",
        "laravel/horizon": "^1.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.5.*",
        "league/flysystem": "~1.0",
        "maatwebsite/excel": "~2.1.0",
        "michelf/php-markdown": "^1.6",
        "ml/json-ld": "1.*",
        "neitanod/forceutf8": "^2.0",
        "nstapelbroek/culpa": "^2.0",
        "pda/pheanstalk": "^3.1",
        "php-http/guzzle6-adapter": "^1.1",
        "phpmailer/phpmailer": "~5.2",
        "phpoffice/phpexcel": "^1.8",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.6",
        "psr/log": "^1.0",
        "pusher/pusher-php-server": "^3.0",
        "roave/security-advisories": "dev-master",
        "shiftonelabs/laravel-cascade-deletes": "^1.0",
        "smajti1/laravel-wizard": "^1.0",
        "spatie/eloquent-sortable": "^3.2",
        "spatie/laravel-artisan-dd": "^2.0",
        "spatie/laravel-fractal": "^5.0",
        "spatie/laravel-translatable": "^2.0",
        "symfony/config": "^3.3",
        "symfony/dom-crawler": "^3.3",
        "symfony/filesystem": "^3.3",
        "symfony/psr-http-message-bridge": "^1.0",
        "techlab/smartwizard": "^4.2",
        "thedevsaddam/laravel-schema": "^2.0",
        "venturecraft/revisionable": "^1.28",
        "vlucas/phpdotenv": "~2.2",
        "willdurand/negotiation": "^2.0",
        "yajra/laravel-datatables-oracle": "^8.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "ImportVocab\\": "plugins/jpAdminGeneratorPlugin/lib/ImportVocab",
            "model\\": "lib/model",
            "apps\\": "apps",
            "App\\": "app/"
        },
        "classmap": [
            "lib/model/",
            "database",
            "tests"
        ],
        "files": [
            "app/helpers.php",
            "app/Helpers/helpers.php"
        ]
    },
    "require-dev":
    {
        "backpack/generators": "^1.1",
        "barryvdh/laravel-debugbar": "^2.1",
        "barryvdh/laravel-ide-helper": "^2.3",
        "filp/whoops": "~2.0",
        "flow/jsonpath": "^0.3.4",
        "fzaninotto/faker": "^1.6",
        "kint-php/kint": "^2.0",
        "laravel/browser-kit-testing": "^2.0",
        "laravel/dusk": "^2.0",
        "mockery/mockery": "0.9.*",
        "mpociot/laravel-test-factory-helper": "^0.4.0",
        "orangehill/iseed": "^2.3",
        "phpunit/phpunit": "~6.0",
        "spatie/laravel-db-snapshots": "^1.0",
        "spatie/phpunit-snapshot-assertions": "^1.0",
        "sstalle/php7cc": "^1.1",
        "tightenco/mailthief": "^0.3.4",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "ImportVocab\\": "plugins/jpAdminGeneratorPlugin/lib/ImportVocab",
            "model\\": "lib/model",
            "apps\\": "apps",
            "App\\": "app",
            "Database\\": "database/",
            "Tests\\": "tests/"
        },
        "classmap": [
            "lib/model/",
            "database",
            "tests"
        ],
        "files": [
            "app/helpers.php",
            "app/Helpers/helpers.php",
            "tests/helpers.php"
        ]
    },
    "extra":
    {
        "laravel":
        {
            "dont-discover": [
                "barryvdh/laravel-ide-helper",
                "backpack/generators",
                "laravel/dusk"
            ]
        }
    },
    "scripts":
    {
        "post-root-package-install" : [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate"
        ]
    },
    "include-path": [
        "lib/model"
    ]
}
